/*********** 1. FILE IMPORTS ************/
/****************************************/
@import "~@index/assets/scss/mixin";
@import "~@index/assets/scss/common";
@import "~@index/assets/scss/_media";


/*********** 2. EXTEND STYLE ************/
/****************************************/



/*********** 3. COMMON VARIABLE *********/
/****************************************/
$min-width:1200px;
$line-color:#eee;
$main-color:#339acd;
$box-bg-color:#fff;
$box-border-color: #dfe2e5;
$spacing:20px;
$header-height:52px;



/*********** 4. HEADER PANEL ************/
/****************************************/
$header-z-index:2080;
$header-el-popover-width:calc(100% - 300px);
$header-el-popover-height: auto;
$header-el-popover-bg-color: #e4e8eb;
.header{
  @include font-adjust(16px, $header-height, #333, left, 100);
  @include position(0, 0, null, 0, fixed);
  height: $header-height; padding: 0 60px 0 15px; background-color: #fff; z-index: $header-z-index;
  border-bottom: 1px solid #f0f0f0; min-width:950px;
  .mark{
    float: left;display: inline-block; width: 185px; margin-right: 20px;
    img{max-width: 23px; display: inline-block; vertical-align: sub; margin-right: 2px;}
    strong{@include font-adjust(18px, $header-height, #333, left, 700)}
  }
  .web-nav{
    display: block; width: 104px; float:left; text-align: center;cursor: pointer; position: relative;
    &:hover, &:focus, &.on{color:#0080c1}
  }
  ul.right{ @include clearfix(); float: right }
  i{position: relative}
  li{
    float: left; padding: 0 10px; margin-left: 10px;cursor: pointer;
    &:hover, span:hover{color: #0080c1}
    span{font-size: 16px;color: #333; display: block }
  }
}
.header-el-popover-cont{
  $left-cont-width:200px;
  $popover-cont-spacing:20px;
  @include clearfix();
  @include position(50px, 110px, null, 210px);
  @include box-shadow(1px, 3px, 5px, #999);
  @at-root .left-menu-shrink &{left: 60px;}
  @at-root .left-menu-shrink.right-menu-open &{left: 60px; right: 280px}
  text-align:left; background-color: #fff;border: 1px solid #ebeef5; line-height: 1; z-index: 2099; overflow: hidden;
  &.fixed{position: fixed;}
  div.main-cont{
    @include clearfix();
    width: calc(100% + 20px); overflow: auto;
  }
  ul.left {
    float: left; width:$left-cont-width;padding: 32px 20px;
    height: $header-el-popover-height;
    li{
      margin:0 0 32px 0; float: none; padding: 0;
      &:nth-child(1){a{color: #000; font-size: 16px;}}
    }
    h2{
      @include font-adjust(16px, 1, #000, null, 100);
    }
    a {
      @include font-adjust(13px, 1.1, null, left, null);
      @include a-style(#333, #f04048)
    }
  }
  div.right {
    float: left; @include calc($minus:#{$left-cont-width}); padding: 0 20px 0 0;
    max-height: $header-el-popover-height; position:relative;  border-left: 1px solid #d3d7da;
    h2{
      padding: 30px 0 30px 20px; line-height: 1;
      a{
        @include a-style();
        @include font-adjust(16px, 1.5, #0080c1, null, 100);
        display: inline-block; margin-right: 30px;}
    }
    h3{
      @include clearfix();
      @include font-adjust(16px, 36px, #fff, left, 700);
      padding-left: 1px; margin:0 0 10px 20px; height: 36px;
      &.bg1{
        background: -webkit-linear-gradient(left, #2d53a1 , #337eb3);
        background: -o-linear-gradient(right, #2d53a1, #337eb3);
        background: -moz-linear-gradient(right, #2d53a1, #337eb3);
        background: linear-gradient(to right, #2d53a1 , #337eb3);
      }
      &.bg2{
        background: -webkit-linear-gradient(left, #24acdf , #3187b5);
        background: -o-linear-gradient(right, #24acdf, #3187b5);
        background: -moz-linear-gradient(right, #24acdf, #3187b5);
        background: linear-gradient(to right, #24acdf , #3187b5);
      }
      span{
        display: inline-block; float: left; width: 80px; height: 30px;margin: 4px 5px 0 0;
        background: url("/static/images/product-logo.png") no-repeat 0;
        &.sol-logo{ background-position: 0 0}
        &.ana-logo{ background-position: 0 -29px}
      }
    }
    .cont{@include clearfix(); position: relative;overflow: hidden; margin-bottom: $popover-cont-spacing}
    h4{@include font-adjust(14px, 1, #0080c1, left, 100); padding-left: $popover-cont-spacing;margin-bottom: 10px;}
    dl{
      @include calc(25%, $popover-cont-spacing);
      background-color: #f8f9fb; padding-bottom: 15px; margin-left: $popover-cont-spacing; float: left; overflow: hidden;
      &:after{content: ''; position: absolute; @include calc(25%, $popover-cont-spacing);background-color: #f8f9fb; height: 100%;}
    }
    dt{@include font-adjust(13px, 34px, #000, left, 100); padding-left: 15px; background-color: #efefef}
    dd{
      @include text-hide();
      padding:0 15px 0 15px;  margin-top: 20px;width: 100%; font-size: 12px;
      a{@include a-style(#666, #f04048);}
    }
  }
}
.header-dropdown-menu-item{
  padding: 0;
  div{
    padding: 0 20px;
  }
}
.header-dropdown-zindex{
  z-index: 2099!important;
}

/*********** 5. LEFT MENU ***************/
/****************************************/
$menu-z-index:1700;
$menu-main-padding: 0 10px 0 20px;
//一级菜单
$menu-first-width: 200px;
$menu-first-bg-color: #2b333e;
$menu-first-title-bg-color:#1a1f25;
$menu-first-title-text-color:#9199a4;
$menu-first-item-title-text-color:#fff;
$menu-first-item-title-bg-color:#252c36;
$menu-first-item-text-color:#b3c8db;
$menu-first-item-hover-text-color:#fff;
$menu-first-item-hover-bg-color:rgba(46, 139, 185, 0.78);
$menu-first-item-focus-text-color:#fff;
$menu-first-item-focus-bg-color:#339ace;
//二级菜单
$menu-second-width: 200px;
$menu-second-bg-color: #d1d5d8;
$menu-second-title-bg-color:#bcc0c3;
$menu-second-title-text-color:#333;
$menu-second-item-title-bg-color:#c9cdd0;
$menu-second-item-title-text-color:#333;
$menu-second-item-text-color:#333;
$menu-second-item-hover-text-color:#0080c1;
$menu-second-item-hover-bg-color:null;
$menu-second-item-focus-text-color:#0080c1;
$menu-second-item-focus-bg-color:#fff;
.sidebar-container {
  //transition: width 0.28s;
  width: 200px; height: 100%; position: fixed; top: 0; bottom: 0; left: 0;  background: #2b333e;z-index: $menu-z-index;
  a { display: inline-block; width: 100%; }
  .svg-icon { margin-right: 16px; }
  .el-menu { border: none; width: 100%; }
}
.scroll-container {
  .scroll-wrapper { position: absolute; width: 100%!important; }
}

.left-menu{
  @include position($header-height, null, 0, 0, relative);
  width: $menu-first-width; background-color: $menu-first-bg-color; text-align: left; white-space: normal;
  label{
    @include font-adjust(13px, 50px, $menu-first-title-text-color, null, 700);
    display: block; padding: $menu-main-padding; background-color: $menu-first-title-bg-color; height: 50px;
    i {
      float: right;cursor:pointer;
      &:hover{color: #fff}
    }
  }
  .menu-first{
    display: block; overflow: auto;
    .menu-first-index > a {
      @include font-adjust(14px, 50px, #fff, null, 100);
      display:block;padding: $menu-main-padding; cursor: pointer; height: 50px;
      &:hover{color: #fff; background-color: rgba(46, 139, 185, 0.78)}
      &.on,  &.router-link-active, &.router-link-exact-active{background-color: #339ace;color: #fff}
    }
    .menu-first-item  > span{
      @include font-adjust(14px, 40px, $menu-first-item-title-text-color, null, 100);
      display:block;padding: 0 10px 0 15px; cursor: pointer; background-color: $menu-first-item-title-bg-color;
    }
    .menu-first-item  > span  i{float: right}
    .menu-first-item-child{
      .menu-first-item-child-title{
        @include font-adjust(13px, 30px, #9199a4, null, 100);
        display: block; height: 30px; padding: $menu-main-padding; background-color: #2f3742; margin-bottom: 1px;
        @at-root .left-menu-shrink &{
          overflow: hidden;
          &:hover{
            position: relative;z-index: 2;width: 200px; overflow: inherit;
          }
        }
      }
      .menu-first-item-child-title_text{margin-left: 30px;}
    }
    .menu-first-item-child-item{
      @include font-adjust(14px, 1, $menu-first-item-text-color, null, 100);
      & > li > span,
      & > li > a{
        display: block;padding: 10px 10px 10px 20px; cursor: pointer; line-height: 30px; color: $menu-first-item-text-color; margin-bottom: 1px;
        &:hover{
          color: $menu-first-item-hover-text-color; background-color: $menu-first-item-hover-bg-color;
        }
        &.on,
        &.router-link-active,
        &.router-link-exact-active{background-color: $menu-first-item-focus-bg-color;color: $menu-first-item-focus-text-color}
        @at-root .left-menu-shrink &:hover{
          width:200px; position: relative; z-index: 1;
          .expend-show{margin-left:20px; display:inline-block;}
        }
         white-space: nowrap; text-overflow: ellipsis; overflow: hidden;
      }
      i{margin-right: 5px; font-size: 20px;}
    }
  }
  .menu-second{
    @include position(52px, null, 0, $menu-second-width, fixed);
    width: $menu-second-width; background-color: $menu-second-bg-color;
    label{
      background-color: $menu-second-title-bg-color; color: $menu-second-title-text-color;
    }
    .menu-second-index > a {
      @include font-adjust(13px, 50px, #333, null, 100);
      display:block; padding: $menu-main-padding; cursor: pointer;
      i{float: right}
      &.router-link-exact-active{color:#0080c1; background-color: #fff};
    }
    .menu-second-item  > span{
      @include font-adjust(13px, 40px, $menu-second-item-title-text-color, null, 700);
      display:block;padding: $menu-main-padding; cursor: pointer; background-color: $menu-second-item-title-bg-color;
      i{float: right;font-size: 16px;}
    }
    .menu-second-item-child {
      margin-left: 0; padding-bottom: 5px;
      li > a{
        @include font-adjust(13px, 50px, $menu-second-item-text-color, null, 100);
        display: block; padding-left: 20px;
        &:hover{color: $menu-second-item-hover-text-color};
        &.router-link-active,
        &.router-link-exact-active{color:$menu-second-item-focus-text-color; background-color: $menu-second-item-focus-bg-color};
      }
    }
  }
  .menu-second-zoom{
    @include font-adjust(14px, 90px, #333);
    @include box-center(null, 90px, fixed);
    display: block;width: 15px; background-color: $menu-second-bg-color; cursor: pointer; color: #333;  left: 200px;
    &:hover{  color: #339acd}
    @at-root .left-menu-shrink .left-menu .menu-second-zoom { left: 60px; }
  }
}



/*********** 6. RIGHT MENU ***************/
/*****************************************/
$right-menu-z-index: 2090;
$right-menu-width: 220px;
$right-menu-title-width: 50px;
$right-menu-color:#000;
$right-menu-bg-color: #f8f8f8;
.right-menu{
  @include position(0, 0, 0, null, fixed);
  width: $right-menu-title-width; z-index:$right-menu-z-index ; background-color: $right-menu-bg-color; overflow: hidden;
  & > div {position:relative; width:100%; height:100%;}
  .right-tabs{
    @include clearfix();
    @include position(0, 0, 0, 0);
  }
  .right-tabs-title{
    width:$right-menu-title-width; height:100%;
    span{
      @include font-adjust(24px, 52px, #3e5a9b, center, 700);
      display:block;width:100%; cursor:pointer;
      @at-root .right-menu-open &.on{background-color:#fff}
    }

  }
  .right-tabs-content{
    @include font-adjust(14px, 1.5, #000, left);
    @include calc($right-menu-width, $right-menu-title-width);
    @include position(0, null, 0, $right-menu-title-width);
    background-color:#fff;height:100%; padding: 10px 10px;
  }
  .label-icon{font-size: 24px;}
  .tab-label{display: block}
  .setting{
    text-align: left;padding-top: 10px;
    dt{
      @include font-adjust(16px, 1.2, $right-menu-color, left, 700);
      margin-bottom: 30px; padding-left: 5px;
    }
    dd{
      color: $right-menu-color;margin-bottom: 20px;height: 25px; line-height: 25px; padding-right: 4px;
      .el-switch{ float: right; height: 25px; line-height: 25px; }
    }
  }
  .history, .collection, .feedback{
    text-align: left;padding-top: 10px;
    dt{
      @include font-adjust(16px, 1.2, $right-menu-color, left, 700);
      margin-bottom: 30px; padding-left: 5px;
    }
    dd{
      margin-bottom: 30px;padding-left: 5px;
      a{@include a-style($right-menu-color, #f04048)}
    }
  }
  .go-header{
    @include font-adjust(14px, 1, #3b5997);
    @include position(null, null, 0, 0);
    display:block; width:$right-menu-title-width; height:$right-menu-title-width; cursor:pointer;
  }
}
.right-menu{
  .open{display: block}
  .close{display: none}
}


/*********** 7. MAIN PANEL ***************/
/*****************************************/
$main-z-index:1600;
$main-padding-footer-height:170px;
$main-bg-color:#e4e7ea;
.main-panel{ margin-left: 200px; margin-right: 50px; background-color: $main-bg-color;z-index: $main-z-index;}
.container{ padding-top: 100px + $spacing; margin: 0; }
.content{ margin: 0 $spacing; }

.left-menu-shrink {
  .left-menu{
    width: 60px;
    & > label > i{float: none}
    .menu-first{
      overflow: inherit;
      .menu-first-item > span{padding: 0 15px; }
    }
    .shrink-show{display: inline-block}
    .expend-show{display: none}
    .menu-second{left: 60px; }
  }
  .sidebar-container{ width: 60px; }
  .main-panel{margin-left: 60px;}
}
.left-menu-shrink.left-submenu-popover {
  .main-panel{margin-left: 260px;}
}
.left-submenu-popover {
  .main-panel{margin-left: 400px;}
}
.left-menu{
  .expend-show{cursor: pointer;}
  a.expend-show{color: #b3c8db;cursor: pointer;  &:hover{color: #fff}}
  .shrink-show{display: none}
}
.right-menu-open{
  .right-menu{
    width: 220px;
    .open{display: none}
    .close{display: block}
  }
  .main-panel{margin-right: 220px;}
  .header{padding-right: 220px;}
}




