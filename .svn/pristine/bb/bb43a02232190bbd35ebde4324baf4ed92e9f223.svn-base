      cite{ @include a-style(#333); cursor: pointer; width: 100%}
      li{
        @include clearfix();
        @include calc(20%, 20px);
        @include transition(all, .4s);
        border: 1px solid #eee; margin: 0 10px 10px; float: left;
        &:hover{
          border-color: #339acd;
          cite{
            color: #339acd;
            &:hover{color: #f04048}
          }
        }
      }
