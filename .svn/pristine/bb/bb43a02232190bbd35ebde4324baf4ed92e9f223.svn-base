/*********** 9. HOME PAGE ***************/
/****************************************/
%home-box{
  @include box-shadow(0, 0, 10px, rgba(153, 153, 153, 0.14));
  @include clearfix();
  background-color: #fff; margin-bottom: $spacing;
}
%home-box-title{
  @include font-adjust(17px, 54px, #003344, left, 100);
  position: relative; padding: 0 30px; height: 54px;
  .right{float: right;}
  i{
    @include font-adjust(20px, 24px, #777, null, 100); cursor: pointer;
    &:hover{color: #0080c1}
  }
}
.el-box-card-rt{
  -webkit-box-shadow:none!important;
  box-shadow:none!important;;
  float: left; width: calc(100%/3 - 14px); margin-right: 20px; margin-bottom: 20px;height: 330px;border: 1px solid $line-color;
  &:nth-child(3n){margin-right: 0}
  .el-card__header{padding: 15px 20px; border: none;}
}
.home{
  padding-top: $header-height + 20px; margin: 0 $spacing;
  .notice{
    @include clearfix();
    h2{
      @include font-adjust(16px, 58px, #000, left, 100);
      i{
        @include font-adjust(18px, 32px, #fff, center, 100);
        @include radius(1234, 4px);
        display: inline-block;width: 32px; height: 32px; margin-right: 10px;
        &.bg-color-red{background-color: #e55353}
        &.bg-color-blue{background-color: #246ba9}
      }
    }
    ul{ @include clearfix(); }
    li.cont-item{
      float: left; padding: 0 20px; margin-right: $spacing; min-height: 185px;
      &:nth-child(1){
        @extend %home-box;
        width: 70%;
      }
      &:nth-child(2){
        @extend %home-box;
        @include calc(30%, $spacing);
        float: right;  margin-right: 0; padding: 0; position: relative;
        h3{
          @include font-adjust(16px, 1.2, #333, left);
          margin-bottom: 10px; background-color:#fafafa; padding: 0 10px;
          span{
            display: inline-block; cursor: pointer; padding: 10px; border-bottom: 1px solid transparent;
            &:hover{color: #0080c1}
            &.on{color: #0080c1; border-color: #0080c1}
          }
        }
        .upcoming{
          padding: 0 20px;
          p{
            @include google-text-hide(3);
            @include font-adjust(14px, 1.4, #000, left, null);
            margin-bottom: 10px; padding-top: 10px;
          }
          .more{
            @include transition();
            @include box-Module(block, 160px, 34px, $border:1px solid #339acd);
            @include font-adjust(14px, 34px, #339acd, center, null);
            position: absolute; bottom: 25px; left: 20px;
            &:hover{background-color: #339acd; color: #fff}
          }
        }
        .inform{
          padding: 0 20px;
          a{
            @include font-adjust(14px, 2, null, left, null);
            @include a-style();
            @include text-hide();
            display: block;
          }
        }
      }
      dl{
        @include clearfix();
        @include transition();
        float: left; padding:0 10px 10px; width: calc(33.3% - 7px); margin-right: 10px; border: 1px solid #eee;
        &:hover{ @include box-shadow(0, 1px, 10px, #ddd);}
        &:last-child{margin-right: 0}
      }
      dt{
        @include font-adjust(12px, 34px, #333, left, 100);
        @include text-hide();
        position: relative;
        a{@include a-style(); }
      }
      dd{
        @include google-text-hide(3);
        @include font-adjust(12px, 1.4, #999, left, 100);
        float: left;
        &:nth-child(2){width: 50px; margin-right:15px;}
        &:nth-child(3){@include calc(100%, 70px)}
      }
    }
    .el-carousel__indicator{ padding: 2px 4px }
    .el-carousel__button{
      @include radius(1234, 50%);
      display: inline-block; width: 10px; height: 10px; background-color: #339acd;
    }
  }

  .product{
    @extend %home-box;
    h2{
      @extend %home-box-title; color: #000; font-weight: 100;
      .right{@include a-style($underLine:none); font-size: 13px; cursor: pointer}
      em{display: inline-block; font-size: 20px; cursor: pointer; vertical-align: bottom}
    }
    & > div{
      @include clearfix();padding: 0 30px 10px;
    }
    @at-root .product-box-card{
      @extend .el-box-card-rt;
    }
    .product-box-card-main{
      width: 100%; height: 276px; padding: 0 20px 10px;
    }
    @at-root .product-box-car-header{color: #003344}
  }

  .data{
    @include clearfix();
    & > li{
      @extend %home-box;
      float: left; height: 304px;
      @media screen and (max-width: 1260px) {
        height: 266px;
      }
      &.left-content,
      &.center-content{
        width: 35.16%; margin-right: $spacing;
        dt{
          @include font-adjust(16px, 44px, #003344, left, 100);
          background-color: #fcfcfc; padding-left: 20px;border-bottom:1px solid #f1f1f1;
        }
        dd{ @include clearfix(); }
        div{
          @include font-adjust(14px, 1.2, #333, left, null);
          @include clearfix();
          display: inline-block; padding: 20px 10px; float: left;
          border-right: 1px solid #f1f1f1;border-bottom:1px solid #f1f1f1; text-align: center;
          &.bd0{border-bottom:none}
          &.month{border-right:1px solid #f1f1f1; width: 60%;}
          &.year{ width: 40%; border-right: none;}
          @media screen and (max-width: 1260px){
            padding: 10px;
          }
        }
        h4{@include font-adjust(14px, 40px, #000, left, 100);padding: 0 20px; clear: both; border-bottom:1px solid #f1f1f1; }
        span{
          @include text-hide();
          display: inline-block; text-align: center; color: #999; font-size: 12px;float: left;
          &.small{width:70px;}
          &.center{@include calc(100%, 140px)}
          &.big{@include calc(100%, 70px)}
        }
        strong{
          @include font-adjust(20px, 30px, #0481c4, center, 100);
          @include text-hide();
          display: block; height:30px; margin-bottom: 5px;
          @media screen and (min-width: 1640px) {
            font-size: 22px;
          }
          @media screen and (max-width: 1420px) {
            font-size: 16px; font-weight: 600;
          }
          @media screen and (max-width: 1260px) {
            font-size: 12px;
          }
        }
        em{
          @include font-adjust(12px, 30px, #666, center, 100);
          display: block; height: 30px;margin-bottom: 5px;
        }
      }
      &.right-content{
        @include calc(29.68%, $spacing * 2);
        padding: 0 20px 0;  overflow: hidden;
        h4{
          @include font-adjust(16px, 44px, #003344, left, 100); margin-bottom:2px;
          a{ @include a-style($underLine:none); font-size: 12px; float: right; }
        }
        li {
          margin-bottom: 5px; line-height: 21px;
          a{
            @include a-style();
            @include text-hide();
            @include calc(100%, 50px);
            display: inline-block;
          }
          cite{float: right; color: #999999;font-size: 13px;}
        }
      }

    }
  }

  .solution{
    @extend %home-box;
    h2{ @extend %home-box-title; }
    ul{ @include clearfix(); padding:0 30px 20px;}
    li{
      width: calc(20% - 16px); margin:0 0 20px 20px; float: left; position: relative; height: 300px; text-align: center; border: 1px solid #eee;
      &:nth-child(5n + 1){margin-left: 0;}
    }
    img {width: 100%; overflow: hidden; height: 128px; display: block; margin-bottom: 15px;}
    em{
      @include position(20px, $left: -10px);
      @include font-adjust(12px, 22px, #fff, center, null);
      display: block; width: 52px;
      &:before{content: ''; position: absolute; left: 0; top: -10px;}
      &.greed{
        background-color: #92ba3d;
        &:before{ @include right-angledTriangle(3, #92ba3d, 5px); }
      }
      &.yellow{
        background-color: #ff6801;
        &:before{ @include right-angledTriangle(3, #ff6801, 5px); }
      }
    }
    i{font-size: 32px;display: inline-block; margin-bottom: 30px;}
    h3{@include font-adjust(16px, 1.5, #333, center, 100);  margin-bottom: 10px;}
    p{
      @include google-text-hide(3);
      @include font-adjust(12px, 1.5, #a7b2b6, center, null);
      -webkit-box-orient:vertical; margin-bottom: 20px; padding: 0 20px; height: 54px;
    }
    a{
      @include transition();
      @include box-Module(block, 110px, 32px, null, null, null, 1px solid #329acd);
      @include font-adjust(14px, 32px, #329acd, center, null);
      @include box-center(110px);
      bottom:20px;
      &:hover, &.on{background-color: #329acd ; color: #fff}
      &.on{
        border: none;
        &:hover{background-color: rgba(50, 154, 205, 0.8); color: #fff; border-color: rgba(50, 154, 205, 0.8)}
      }
    }
  }
  .analytics{
    @extend %home-box;
    h2{
      @extend %home-box-title;
      font{margin-right: 60px;}
      span{
        @include font-adjust(15px, null, #003344, center, 100);
        display: inline-block; padding: 0 20px; cursor: pointer;
        &:hover{color: #329acd}
        &.on{border-bottom: 1px solid #329acd; color: #329acd}
      }
    }
    ul{
      @include clearfix();padding: 0 30px 30px;
      & > li {
        float: left; margin: 20px 20px 0 0; width: calc(25% - 15px);  height: 224px; position: relative;
        &:nth-child(4n){margin-right: 0}
        div{
          @include clearfix();border: 1px solid #eee;background-color: #fff; height: 224px; min-height: 224px; width:100%;
        }
        h4{
          @include font-adjust(15px, 20px, #000, left, 100);
          padding: 15px 20px; background-color: #f7f7f7;border-bottom: 1px solid #eee;
        }
        em{
          @include text-hide();
          font-weight:100; font-size: 12px; color: #a8b1b6; display: block;
        }
        .more, .hide{
          @include a-style($underLine:none);
          float: right; margin-right: 20px; margin-bottom: 10px; cursor: pointer;
        }
        .hide{
          display: none;
        }
      }
    }
    ol{
      @include clearfix(); padding: 20px 20px 0; height: 110px; overflow: hidden; width: 100%;
      li{
        @include transition(all, .05s);
        @include clearfix();
        @include text-hide();
        margin-bottom: 10px;
      }
      a {@include a-style($underLine:none);@include text-hide(); max-width: 100%;float:left; display: inline-block; }
      i {
        float:left;display: inline-block; margin-right: 5px; width: 20px;
      }
    }

    li.expend{
      & > div {
        @include box-shadow(1px, 1px, 10px, #cfcfcf);
        width: 680px; position: absolute; left: 0; z-index: 1; height: auto;
      }
      &:nth-child(4n), &:nth-child(4n - 1){
        & > div {right: 0; left: inherit}
      }
      li { width: calc(33% - 25px);display: inline-block; margin-right: 20px; }
      ol {height: auto}
      .more{display: none}
      .hide{display: block}
    }
  }
  // END HOME PAGE
}



