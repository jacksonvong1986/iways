rands['previous_level']\n          this.models = this.brands['list']\n          //获取当前选中的条件\n          this.selected_brand_id = this.brands.previous_level.id\n          this.selected_brand_name = this.brands.previous_level.name\n          this.selected_model_id = []\n          //获取当前需要提交的条件\n          this.push_model = this.selected_all_model\n          let selected_model_id = this.push_model.map( item => {\n            return item.id\n          })\n          this.transferGetMonthForecast(this.push_model)\n          this.getMonthForecastHeader({\n            manage_type_id:1,\n            brand_id: this.selected_brand_id\n          })\n        }\n\n      }\n    },\n    components:{\n      BiBar,BiPanel,BiEmpty,BiProgress\n    }\n  }\n</script>\n\n<style lang=\"scss\" type=\"text/scss\" scoped>\n\n</style>\n\n\n\n// WEBPACK FOOTER //\n// src/module/client/page/zhiku/cyc/sales/provinceAlarmDetail.vue","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',[_c('bi-bar',{attrs:{\"columnTitle\":\"预警详情\"}},[_c('template',{slot:\"button\"},[_c('span',{staticClass:\"mgr20 mgl10\"},[_vm._v(\"时间：\"+_vm._s(_vm.year_month))]),_vm._v(\" \"),_c('span',{staticClass:\"mgr20\"},[_vm._v(\"管理维度：\"+_vm._s(_vm.dimension_name))]),_vm._v(\" \"),_c('span',{staticClass:\"mgr20\"},[_vm._v(\"管理类型：\"+_vm._s(_vm.manage_type))])]),_vm._v(\" \"),_c('template',{slot:\"right\"},[_c('el-breadcrumb',{attrs:{\"separator\":\"/\"}},[_c('span',{staticClass:\"bread-legend\"},[_vm._v(\"当前位置：\")]),_vm._v(\" \"),_c('el-breadcrumb-item',{attrs:{\"to\":{ path: '/' }}},[_vm._v(\"概览主页\")]),_vm._v(\" \"),_c('el-breadcrumb-item',{attrs:{\"to\":{ path: '/zhiku/xl' }}},[_vm._v(\" 销量目标管理\")]),_vm._v(\" \"),_c('el-breadcrumb-item',[_vm._v(\" 省份 - 监测预警\")])],1)],1)],2),_vm._v(\" \"),_c('div',{staticClass:\"content cyc-xl-targetList\"},[(_vm.newstTarget)?[_c('bi-panel',{attrs:{\"operating\":false}},[_c('div',{staticClass:\"targetList-title\",attrs:{\"slot\":\"title\"},s